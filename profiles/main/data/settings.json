    "gitlens.defaultDateShortFormat": "D MMM YYYY",
    // let's see all options and its default, guys
    "editor.fontSize": 14,
    "editor.tabSize": 4,
    "editor.renderWhitespace": "selection",
    "editor.cursorStyle": "line",
    "editor.multiCursorModifier": "alt",
    "editor.insertSpaces": true,
    "editor.wordWrap": "off",
