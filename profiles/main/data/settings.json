{
    "accessibility.dimUnfocused.enabled": false,
    "better-comments.tags": [
        // default tags frpm better-comments
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": true
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "TODO",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        // my personal tag(s)
        {
            "tag": "\":",
            "color": "#309EC4",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": true
        },
        // from `todo-tree`
        {
            "tag": "BUG",// what for ya ini teh 0
            // meureun buat kasih tau apa yang ngebug nya [831]
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "HACK",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "FIXME", // what for ya ini teh 1
            // [831] nah kalo ini ngasih tau gimana implementasi nya buat ngehilangin bug eta
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "XXX", // what for ya ini teh 2
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "[ ]",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "[x]",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "LEGACY",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
    ],
    "dev.containers.dockerPath": "podman",
    "diffEditor.experimental.showMoves": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.codeActionsOnSave": {},
    "editor.fontFamily": "'JetBrainsMono Nerd Font', 'RobotoMono Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontLigatures": true,
    "editor.inlineSuggest.enabled": true,
    "editor.rulers": [
        80,
        88,
        120,
    ],
    "editor.stickyScroll.enabled": true,
    "editor.tabCompletion": "on",
    "explorer.confirmUndo": "verbose",
    "extensions.autoUpdate": false,
    "files.associations": {
        "*.code-workspace.dist": "jsonc",
        ".git-blame-ignore-revs": "properties",
        "*.nix": "properties",
        "TAG_EDITMSG": "git-commit"
    },
    "files.autoSave": "afterDelay",
    "files.exclude": {
