        80,
        88,
        120
    ],
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.wordWrap": "off",
    "explorer.confirmUndo": "verbose",
    "extensions.autoUpdate": false,
